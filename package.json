{
  "name": "react-mobx-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "build": "CI=true yarn test && react-app-rewired build --scripts-version react-scripts-ts",
    "lint": "yarn format && tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix && tsc",
    "format": "prettier --write 'src/**/*.ts*'"
  },
  "dependencies": {
    "@reach/router": "1.1.1",
    "axios": "0.18.0",
    "emotion": "9.2.6",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "mobx-react-devtools": "6.0.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-scripts-ts": "2.17.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/node": "10.5.7",
    "@types/react": "16.4.7",
    "@types/react-dom": "^16.0.7",
    "babel": "6.23.0",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-preset-mobx": "1.0.3",
    "prettier": "1.14.2",
    "react-app-rewire-mobx": "1.0.8",
    "react-app-rewired": "1.5.2",
    "react-testing-library": "5.0.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-eslint-rules": "5.3.1",
    "typescript": "3.0.1"
  }
}
