{
  "name": "react-mobx-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "test": "yarn lint && react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "build": "CI=true yarn test && react-app-rewired build --scripts-version react-scripts-ts",
    "lint": "yarn format && tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix && tsc",
    "format": "prettier --write 'src/**/*.ts*'"
  },
  "dependencies": {
    "@reach/router": "^1.0.5",
    "axios": "^0.18.0",
    "emotion": "^9.1.3",
    "mobx": "^4.2.0",
    "mobx-react": "^5.1.2",
    "mobx-react-devtools": "^5.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts-ts": "^2.16.0"
  },
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "babel": "^6.23.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-preset-mobx": "^1.0.2",
    "prettier": "^1.13.0",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "tslint-config-prettier": "^1.12.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.1"
  }
}
