{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "no-var-requires": false,
    "no-floating-promises": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false
  }
}